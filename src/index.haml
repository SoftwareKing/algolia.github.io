                      %svg.icon
                        %use{xlink:href: "#icon-github"}
              .spacer10
              .project-wrapper
                .project-desc
                  %a{href: "#"}Swag
                   
          .col-sm-3
            .project-wrapper.big
                .project-desc
                  Tools

          .col-sm-6
              .project-wrapper.newsletter
                .project-desc
                  %a{href: "https://github.com/algolia/expect-jsx"}Newsletter
            .spacer10
            .row
              .col-sm-6
                .project-wrapper.newsletter
                  .project-desc
                    %a{href: "https://github.com/algolia/expect-jsx"}Talks
              .col-sm-6
                .project-wrapper.newsletter
                  .project-desc
                    %a{href: "https://github.com/algolia/expect-jsx"}Public Datasets

            
