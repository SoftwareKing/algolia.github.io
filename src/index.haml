                    .project-header TOOL
                    .project-desc
                      %svg.icon{height: 40}
                        %use{xlink:href: "#icon-github"}
                      %a.project-title{href: "https://github.algolia.com"} Github Search
                      .spacer10
                    .project-footer

            .spacer10
            .row
              .col-sm-6
                %figure.effect-apollo.project-wrapper.supersearch
                  %figcaption
                    .project-header DEMO
                    .project-desc
                      %svg.icon{height: 50}
                        %use{xlink:href: "#icon-superbowl"}
                      %a.project-title{href: "https://community.algolia.com/supersearch"} SuperSearch
                      .spacer10
                    .project-footer
              .spacer10.visible-xs
              .col-sm-6
                %figure.effect-apollo.project-wrapper
                  %figcaption
                    .project-header NEED HELP?
                    .project-desc
                      %img.icon{src: "img/illus-stackoverflow.svg", width: 40}
                      %a.project-title{href: "http://stackoverflow.com/questions/tagged/algolia"} ASK StackOverflow
        .spacer10
        .row
          .col-sm-3
              %figure.effect-apollo.project-wrapper.searchbox
                %figcaption
                .project-header TOOL
                  .project-desc
                    .spacer50
                    %a.project-title{href: "http://shipow.github.io/searchbox/"} SearchBox
                    %p.text-center A generator for search input.
              .spacer10
              %figure.effect-apollo.project-wrapper.swag
                %figcaption
                  .project-header SWAG
                  .project-desc
                    %svg.icon{height: 40}
                      %use{xlink:href: "#icon-tshirt"}
                    %a{href: "#", class: "button full"} Get your free t-shirt

          .spacer10.visible-xs
          .col-sm-3
            %figure.effect-apollo.project-wrapper.big.expectjs
              %figcaption
                .project-header TOOL
                .project-desc
                  %svg.icon{height: 120}
                    %use{xlink:href: "#icon-react"}
                  .spacer20
                  %a.project-title{href: "https://github.com/algolia/expect-jsx"} Expect JSX
                  %p toEqulJSX for mjackson/expect
          .spacer10.visible-xs
          .col-sm-6
            %figure.effect-apollo.project-wrapper.newsletter
              %figcaption
                .project-header LEARN
                .project-desc
                  #mc_embed_signup
                    %form#mc-embedded-subscribe-form.validate{action: "//algolia.us3.list-manage.com/subscribe/post?u=f7b8acc6ec97794a08484ca9b&amp;id=285b80e148", method: "post", name: "mc-embedded-subscribe-form", novalidate: ""}
                      #mc_embed_signup_scroll
                        .input-group
                          %input#mce-EMAIL{ name: "EMAIL", placeholder: "Email Address", type: "email", value: ""}
                          %button#mc-embedded-subscribe.button{ type: "submit"}
                            Subscribe
                        #mce-responses.clear
                          #mce-error-response.response{style: "display:none"}
                          #mce-success-response.response{style: "display:none"}
                        / real people should not fill this in and expect good things - do not remove this or risk form bot signups
                        %div{style: "position: absolute; left: -5000px;"}
                          %input{name: "b_f7b8acc6ec97794a08484ca9b_285b80e148", tabindex: "-1", type: "text", value: ""}/
                .spacer15
                %span.project-title Newsletter
                %p.text-center Learn how to build a delightful  search experience
            .spacer10
            .row
              .col-sm-6
                %figure.effect-apollo.project-wrapper.talks
                  %figcaption
                    .project-header LEARN
                    .project-desc
                      %svg.icon{height: 40}
                        %use{xlink:href: "#icon-talks"}
                      %a.project-title{href: "#"} Talks
                    .spacer10
              .spacer10.visible-xs
              .col-sm-6
                %figure.effect-apollo.project-wrapper.publicdatas
                  %figcaption
                    .project-header LEARN
                    .project-desc
                      %svg.icon{height: 30}
                        %use{xlink:href: "#icon-index"}
                      %a.project-title{href: "#"} Public Datasets
                    .spacer10


